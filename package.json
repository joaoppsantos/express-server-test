{
  "name": "express-server-test",
  "version": "1.0.0",
  "description": "Simple server and client using express and typescript",
  "main": "index.js",
  "scripts": {
    "server:run": "tsc src/server/index.ts && node src/server/index.js",
    "client:run": "tsc src/client/index.ts && node src/client/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaoppsantos/express-server-test.git"
  },
  "author": "Jo√£o Santos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaoppsantos/express-server-test/issues"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 80,
    "semi": true,
