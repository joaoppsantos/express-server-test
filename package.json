    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "bracketSpacing": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "axios": "^1.2.3",
    "csv-parser": "^3.0.0",
    "express": "^4.18.2"
